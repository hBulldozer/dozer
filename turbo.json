{
  "$schema": "https://turbo.build/schema.json",
  "globalEnv": [
    "CI",
    "NODE_ENV"
  ],
  "globalDependencies": [
    ".eslintignore",
    ".eslint.js",
    ".npmrc",
    ".nvmrc",
    ".prettierignore",
    ".prettierrc.js",
    "tsconfig.json"
  ],
  "pipeline": {
    "db:init": {
      "outputs": ["prisma/**/*"],
      "cache": false
    },
    "db:pull": {
      "outputs": ["prisma/**/*"],
      "dependsOn": ["db:init"],
      "cache": false,
      "env": ["DATABASE_URL"]
    },
    "db:generate": {
      "dependsOn": [],
      "cache": false
    },
    "db:push": {
      "cache": false
    },
    "build": {
      "dependsOn": ["^db:generate", "^build", "db:generate"],
      "env": [
        "ROOT_URL",
        "SWAP_URL",
        "EARN_URL",
        "DATABASE_URL"
      ],
      "outputs": [
        ".vercel/**/*",
        ".next/**/*",
        ".mesh/**/*",
        "dist/**/*",
        "typechain/**/*",
        "artifacts/**/*",
        "cache/**/*",
        "public/**/*",
        "storybook-static/**/*"
      ]
    },
    "//#build-config": {
      "dependsOn": [],
      "outputs": ["dist/**/*"],
      "inputs": []
    },
    "//#build-packages": {
      "dependsOn": [],
      "outputs": ["dist/**/*"],
      "inputs": []
    },
    "lint": {
      "dependsOn": ["^build"],
      "outputs": []
    },
    "start": {
      "dependsOn": ["^build"],
      "cache": false,
      "persistent": true
    },
    "dev": {
      "dependsOn": ["^build"],
      "cache": false,
      "persistent": true
    },
    "clean": {
      "cache": false
    }
  }
}
