{
  "$schema": "https://turbo.build/schema.json",
  "globalEnv": [
    "CI",
    "NODE_ENV"
  ],
  "globalDependencies": [
    ".eslintignore",
    ".eslint.js",
    ".npmrc",
    ".nvmrc",
    ".prettierignore",
    ".prettierrc.js",
    "tsconfig.json"
  ],
  "tasks": {
    "db:init": {
      "outputs": [
        "prisma/**/*"
      ],
      "cache": false
    },
    "db:pull": {
      "outputs": [
        "prisma/**/*"
      ],
      "dependsOn": [
        "db:init"
      ],
      "cache": false,
      "env": [
        "DATABASE_URL"
      ]
    },
    "db:generate": {
      "dependsOn": [],
      "cache": false
    },
    "db:push": {
      "cache": false
    },
    "build": {
      "dependsOn": [
        "^db:generate",
        "^build",
        "db:generate"
      ],
      "env": [
        "ROOT_URL",
        "SWAP_URL",
        "EARN_URL",
        "POSTGRES_PRISMA_URL",
        "DATABASE_URL",
        "API_KEY",
        "ZEALY_CONNECT_SECRET",
        "ZEALY_API_KEY",
        "NEXT_PUBLIC_LOCAL_NODE_URL",
        "NEXT_PUBLIC_LOCAL_EXPLORER_URL",
        "NEXT_PUBLIC_PUBLIC_NODE_URL",
        "LOCAL_WALLET_MASTER_URL",
        "LOCAL_WALLET_USERS_URL",
        "LOCAL_WALLET_MASTER_ADDRESS",
        "MATOMO_API_KEY",
        "NEXT_PUBLIC_MATOMO_URL",
        "NEXT_PUBLIC_MATOMO_SITE_ID",
        "WALLET_API_KEY",
        "WALLET_ID",
        "LPBLUEPRINT",
        "OASISBLUEPRINT",
        "BETBLUEPRINT",
        "SUPABASE_ANON_KEY",
        "NEXT_PUBLIC_POOL_MANAGER_BLUEPRINT_ID",
        "NEXT_PUBLIC_POOL_MANAGER_CONTRACT_ID",
        "NEXT_PUBLIC_OASIS_CONTRACT_IDS",
        "NEXT_PUBLIC_BRIDGED_TOKEN_UUIDS",
        "NEXT_PUBLIC_OASIS_BLUEPRINT_ID"
      ],
      "outputs": [
        ".vercel/**/*",
        ".next/**/*",
        ".mesh/**/*",
        "dist/**/*",
        "typechain/**/*",
        "artifacts/**/*",
        "cache/**/*",
        "public/**/*",
        "storybook-static/**/*"
      ]
    },
    "//#build-config": {
      "dependsOn": [],
      "outputs": [
        "dist/**/*"
      ],
      "inputs": []
    },
    "//#build-packages": {
      "dependsOn": [],
      "outputs": [
        "dist/**/*"
      ],
      "inputs": []
    },
    "lint": {
      "dependsOn": [
        "^build"
      ],
      "outputs": []
    },
    "start": {
      "dependsOn": [
        "^build"
      ],
      "cache": false,
      "persistent": true
    },
    "dev": {
      "dependsOn": [
        "^build",
        "^db:generate"
      ],
      "env": [
        "ROOT_URL",
        "SWAP_URL",
        "EARN_URL",
        "POSTGRES_PRISMA_URL",
        "DATABASE_URL",
        "API_KEY",
        "ZEALY_CONNECT_SECRET",
        "ZEALY_API_KEY",
        "NEXT_PUBLIC_LOCAL_NODE_URL",
        "NEXT_PUBLIC_LOCAL_EXPLORER_URL",
        "NEXT_PUBLIC_PUBLIC_NODE_URL",
        "LOCAL_WALLET_MASTER_URL",
        "LOCAL_WALLET_MASTER_ADDRESS",
        "LOCAL_WALLET_USERS_URL",
        "MATOMO_API_KEY",
        "NEXT_PUBLIC_MATOMO_URL",
        "NEXT_PUBLIC_MATOMO_SITE_ID",
        "WALLET_API_KEY",
        "WALLET_ID",
        "LPBLUEPRINT",
        "OASISBLUEPRINT",
        "BETBLUEPRINT",
        "SUPABASE_ANON_KEY",
        "NEXT_PUBLIC_POOL_MANAGER_BLUEPRINT_ID",
        "NEXT_PUBLIC_POOL_MANAGER_CONTRACT_ID",
        "NEXT_PUBLIC_OASIS_CONTRACT_IDS",
        "NEXT_PUBLIC_BRIDGED_TOKEN_UUIDS",
        "NEXT_PUBLIC_OASIS_BLUEPRINT_ID"
      ],
      "cache": false,
      "persistent": true
    },
    "clean": {
      "cache": false
    }
  }
}
