{
  "name": "@dozer/database",
  "description": "Dozer Database",
  "version": "0.0.0",
  "dependencies": {
    "@prisma/client": "5.16.2",
    "ioredis": "5.4.1",
    "prisma-redis-middleware": "4.8.0"
  },
  "devDependencies": {
    "@dozer/eslint-config": "workspace:*",
    "@dozer/jest-config": "workspace:*",
    "@dozer/prettier-config": "workspace:*",
    "@dozer/typescript-config": "workspace:*",
    "@types/jest": "^29.5.11",
    "@types/node": "20.14.10",
    "dotenv": "16.4.5",
    "eslint": "*",
    "jest": "29.7.0",
    "prisma": "5.16.2",
    "tsup": "8.1.0",
    "tsx": "^4.7.2",
    "typescript": "^5.3.3"
  },
  "files": [
    "dist/**"
  ],
  "jest": {
    "preset": "@dozer/jest-config/node"
  },
  "keywords": [
    "database",
    "dozer"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "private": true,
  "repository": {
    "type": "git",
    "directory": "packages/database"
  },
  "scripts": {
    "build": "tsc",
    "check": "tsc --pretty --noEmit",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "db:generate": "prisma generate",
    "db:pull": "prisma db pull",
    "db:push": "prisma db push --skip-generate",
    "dev": "tsup src/index.ts --format esm,cjs --watch --dts",
    "generate": "prisma generate",
    "lint": "TIMING=1 eslint src --fix",
    "postinstall": "prisma generate",
    "prebuild": "npm run db:generate",
    "predev": "npm run db:generate",
    "prepublishOnly": "pnpm build",
    "seed": "dotenv -- tsx src/seed_db.ts",
    "studio": "prisma studio",
    "test": "jest --passWithNoTests"
  },
  "source": "src/index.ts",
  "type": "module",
  "types": "dist/index.js",
  "typings": "dist/index.d.ts"
}
