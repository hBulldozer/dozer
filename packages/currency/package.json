{
  "name": "@dozer/currency",
  "version": "0.0.0",
  "description": "Dozer Currency",
  "keywords": [
    "dozer",
    "currency",
    "token",
    "native"
  ],
  "repository": {
    "type": "git",
    "directory": "packages/currency"
  },
  "license": "MIT",
  "author": "Dozer",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "source": "src/index.ts",
  "typings": "dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts",
    "check": "tsc --pretty --noEmit",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "dev": "tsup src/index.ts --format esm,cjs --watch --dts",
    "lint": "TIMING=1 eslint src --fix",
    "prepublishOnly": "pnpm build",
    "test": "jest --passWithNoTests"
  },
  "jest": {
    "preset": "@dozer/jest-config/node"
  },
  "dependencies": {
    "@dozer/math": "workspace:*",
    "@dozer/chain": "workspace:*",
    "lodash.flatmap": "4.5.0",
    "tiny-invariant": "^1.3.1"
  },
  "devDependencies": {
    "@dozer/eslint-config": "workspace:*",
    "@dozer/jest-config": "workspace:*",
    "@dozer/prettier-config": "workspace:*",
    "@dozer/typescript-config": "workspace:*",
    "@types/jest": "^29.5.11",
    "@types/lodash.flatmap": "4.5.7",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "eslint": "8.56.0",
    "jest": "29.7.0",
    "tsup": "8.0.1",
    "typescript": "5.3.3"
  }
}
